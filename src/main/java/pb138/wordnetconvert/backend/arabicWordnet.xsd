<xs:schema version="1.0"
	   xmlns:xs="http://www.w3.org/2001/XMLSchema"
	   elementFormDefault="qualified">
    
    <xs:element name="wordnet">
	<xs:complexType>
	    <xs:sequence>
		<xs:element name="item" minOccurs="0" maxOccurs="unbounded">
		    <xs:complexType>
			<xs:attribute name="itemid" type="xs:string" use="required"/>
			<xs:attribute name="type" type="xs:string" use="required"/>
			<xs:attribute name="POS" type="xs:string" use="required"/>
		    </xs:complexType>
		</xs:element>
		<xs:element name="word" minOccurs="0" maxOccurs="unbounded">
		    <xs:complexType>
			<xs:attribute name="value" type="xs:string" use="required"/>
			<xs:attribute name="synsetid" type="xs:string" use="required"/>
		    </xs:complexType>
		</xs:element>
	    </xs:sequence>
	    <xs:attribute name="version" type="xs:integer"/>
	</xs:complexType>
    </xs:element>

</xs:schema>
